<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:RTFTDIF.Client.Resources.Styles">

    <FontFamily x:Key="BoostLightSSK">pack://application;,,,/Resources/Fonts/#BoostLightSSK</FontFamily>
    <FontFamily x:Key="BankGothicLightBT">pack://application;,,,/Resources/Fonts/#Bank Gothic Light BT</FontFamily>
    <FontFamily x:Key="ElfarNormalG98">pack://application;,,,/Resources/Fonts/#Elfar Normal G98</FontFamily>

    <Style x:Key="BaseStyleFont" TargetType="{x:Type Control}">
        <!--<Setter Property="FontFamily" Value="{StaticResource ElfarNormalG98}"></Setter>-->
    </Style>

    <Style TargetType="{x:Type Button}" BasedOn="{StaticResource BaseStyleFont}"></Style>
    <Style TargetType="{x:Type TextBox}" BasedOn="{StaticResource BaseStyleFont}"></Style>
</ResourceDictionary>